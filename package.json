{
  "packageManager": "pnpm@8.15.8",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev",
    "postbuild": "NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs && node ./scripts/generate-sitemap.mjs",
    "prebuild": "contentlayer build",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "format:write": "prettier --ignore-path .gitignore --write .",
    "format:check": "prettier --ignore-path .gitignore --check ."
  },
  "dependencies": {
    "contentlayer": "0.3.4",
    "date-fns": "2.30.0",
    "next": "13.5.6",
    "next-contentlayer": "0.3.4",
    "next-themes": "0.3.0",
    "preact": "10.21.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reading-time": "1.5.0",
    "rehype-pretty-code": "^0.13.0",
    "style9": "0.18.2",
    "style9-webpack": "^0.5.1",
    "rehype": "13.0.1",
    "rehype-autolink-headings": "7.1.0",
    "rehype-slug": "6.0.0",
    "remark-gfm": "3.0.1",
    "rss": "1.2.2"
  },
  "devDependencies": {
    "@types/node": "20.12.11",
    "@types/react": "18.3.1",
    "eslint": "9.2.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.1.0",
    "globby": "14.0.1",
    "prettier": "3.2.5",
    "typescript": "5.4.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "@opentelemetry/api": "1.1.0"
      }
    }
  }
}
