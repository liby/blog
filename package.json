{
  "packageManager": "pnpm@7.2.1",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.1.0",
    "yarn": ">2 <1",
    "npm": ">2 <1"
  },
  "scripts": {
    "dev": "next dev",
    "postbuild": "NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs && node ./scripts/generate-sitemap.mjs",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "format:write": "prettier --ignore-path .gitignore --write .",
    "format:check": "prettier --ignore-path .gitignore --check ."
  },
  "dependencies": {
    "contentlayer": "0.2.5",
    "date-fns": "2.28.0",
    "next": "12.1.6",
    "next-contentlayer": "0.2.5",
    "next-themes": "0.2.0",
    "preact": "10.7.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "reading-time": "1.5.0",
    "style9": "0.13.3"
  },
  "devDependencies": {
    "@types/node": "17.0.36",
    "@types/react": "18.0.12",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "globby": "13.1.2",
    "next-transpile-modules": "9.0.0",
    "prettier": "2.6.2",
    "rehype": "12.0.1",
    "rehype-autolink-headings": "6.1.1",
    "rehype-code-titles": "1.1.0",
    "rehype-prism-plus": "1.4.0",
    "rehype-slug": "5.0.1",
    "remark-gfm": "3.0.1",
    "rss": "1.2.2",
    "typescript": "4.7.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "@opentelemetry/api": "1.1.0"
      }
    }
  }
}
