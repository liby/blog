{
  "packageManager": "pnpm@8.7.4",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev",
    "postbuild": "NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs && node ./scripts/generate-sitemap.mjs",
    "prebuild": "contentlayer build",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "format:write": "prettier --ignore-path .gitignore --write .",
    "format:check": "prettier --ignore-path .gitignore --check ."
  },
  "dependencies": {
    "contentlayer": "0.3.4",
    "date-fns": "2.30.0",
    "next": "13.4.19",
    "next-contentlayer": "0.3.4",
    "next-themes": "0.2.1",
    "preact": "10.17.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "style9": "0.18.2",
    "style9-webpack": "^0.5.1"
  },
  "devDependencies": {
    "@types/node": "18.17.14",
    "@types/react": "18.2.21",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.0.0",
    "globby": "13.2.2",
    "next-transpile-modules": "10.0.1",
    "prettier": "3.0.3",
    "rehype": "13.0.1",
    "rehype-autolink-headings": "6.1.1",
    "rehype-code-titles": "1.2.0",
    "rehype-prism-plus": "1.6.3",
    "rehype-slug": "6.0.0",
    "remark-gfm": "3.0.1",
    "rss": "1.2.2",
    "typescript": "5.2.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "@opentelemetry/api": "1.1.0"
      }
    }
  }
}
